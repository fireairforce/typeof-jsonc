{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb"
  ],
  "jsRules": {
    "eofline": true,
    "object-literal-sort-keys": false,
    "align": false,
    "ter-arrow-parens": [true, "as-needed"]
  },
  "rules": {
    "import-name": false,
    "object-literal-sort-keys": false,
    "align": false,
    "ordered-imports": false,
    "ter-arrow-parens": [true, "as-needed"]
  },
  "rulesDirectory": []
}
