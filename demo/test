{
  "barr": [ // aaa
    "aaa",
    "bbb"
  ],
  "name": "lanfeng", // this is name
  // this is demo
  "demo": {
    "hello": "world"
  },
  /** this is arr */
  "arr": [{
    "age": 1
  }, 2],
  /**
  * Demonstrate how to modify the source code before the parser sees it.
  *
  * @module plugins/commentConvert
  */
  a: 'hello',
  b: ['a', 'b']
}
